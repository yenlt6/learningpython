    "# Remove special symbols/Punctuation from a given string\r\n",
    "import re\r\n",
    "\r\n",
    "str1 = \"/*Jon is @developer & musician\"\r\n",
    "print(\"Original string is \", str1)\r\n",
    "\r\n",
    "str2 = re.sub(r'[^\\w\\s]', '', str1)\r\n",
    "print(\"New string is \", str2)"
